<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE web-app SYSTEM "web-app.dtd">
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:web="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>textinput</servlet-name>
    <jsp-file>/textinput.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>textinput</servlet-name>
    <url-pattern>/textinput</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>TextServlet</servlet-name>
    <servlet-class>text.TextServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>TextServlet</servlet-name>
    <url-pattern>/TextServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>textoutput</servlet-name>
    <jsp-file>/textoutput.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>textoutput</servlet-name>
    <url-pattern>/textoutput</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>checkinput</servlet-name>
    <jsp-file>/checkinput.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>checkinput</servlet-name>
    <url-pattern>/checkinput</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CheckServlet</servlet-name>
    <servlet-class>check.CheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CheckServlet</servlet-name>
    <url-pattern>/CheckServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>checkoutput</servlet-name>
    <jsp-file>/checkoutput.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>checkoutput</servlet-name>
    <url-pattern>/checkoutput</url-pattern>
  </servlet-mapping>  
</web-app>